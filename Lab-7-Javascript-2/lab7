                let score=80
                function calculateGrade(){
                        if (score < 60){
                            console.log("Grade:F")     
                            } 
                            else if (score < 70) {
                                    console.log("Grade:D")
                                    } 
                            else if (score < 80) {
                                    console.log("Grade:C")
                                } 
                            else if (score < 90) {
                                    console.log("Grade:B")
                            } 
                            else (score>90); {
                                    console.log("Grade:A")
                            }
                        }

                
